{
  "name": "agent-task-manager",
  "version": "1.0.0",
  "description": "## Introduction",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "node dist/server.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "db:generate": "cross-env TS_NODE_PROJECT=./src/db/tsconfig.json npm run typeorm -- migration:generate src/db/migrations/InitialSchema -d src/db/data-source.ts",
    "db:migrate": "cross-env TS_NODE_PROJECT=./src/db/tsconfig.json npm run typeorm -- migration:run -d src/db/data-source.ts",
    "db:drop": "rm -f src/db/agent-task-manager.sqlite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prompttosoftware/agent-task-manager.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/prompttosoftware/agent-task-manager/issues"
  },
  "homepage": "https://github.com/prompttosoftware/agent-task-manager#readme",
  "dependencies": {
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.24",
    "zod": "^3.25.61"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.0.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
