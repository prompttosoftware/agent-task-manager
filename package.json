{
  "name": "agent-task-manager",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "db:migrate": "npx typeorm -- -d src/db/data-source.ts migration:run",
    "db:generate": "npm run build && ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate ./src/db/migrations/$npm_config_name --dataSource ./src/db/data-source.ts",
    "test": "jest",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "db:seed": "ts-node src/db/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.11.20",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "multer": "^2.0.1",
    "pg": "^8.11.3",
    "pino": "^8.19.1",
    "pino-pretty": "^10.3.1",
    "reflect-metadata": "^0.2.1",
    "typeorm": "^0.3.20",
    "uuid": "^11.1.0",
    "zod": "^3.22.4"
  }
}
